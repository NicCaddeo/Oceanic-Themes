				"string.quoted.docstring.raw.multi.python"
			],
			"settings": {
				"foreground": "#888888",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#0be3ff"
			}
		},
