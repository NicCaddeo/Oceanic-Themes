        {
            "scope": "storage.type",
            "settings": {
                "foreground": "#7eb8d6"
            }
        },
        {
            "scope": "storage.modifier",
            "settings": {
                "foreground": "#7eb8d6"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#f0b885"
            }
        },
        {
            "scope": "string.tag",
            "settings": {
