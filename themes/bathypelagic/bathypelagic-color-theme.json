            "settings": {
                "foreground": "#7ed6c6"
            }
        },
        {
            "scope": "storage",
            "settings": {
                "foreground": "#4dc4d9"
            }
        },
        {
            "scope": "storage.type",
            "settings": {
                "foreground": "#4dc4d9"
            }
        },
        {
            "scope": "storage.modifier",
            "settings": {
                "foreground": "#4dc4d9"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#d4a584"
            }
        },
        {
            "scope": "string.tag",
            "settings": {
                "foreground": "#c47f8f"
            }
        },
        {
            "scope": "string.value",
            "settings": {
                "foreground": "#f5f5c8"
            }
        },
        {
            "scope": "string.regexp",
            "settings": {
                "foreground": "#c47f8f"
            }
        },
        {
            "name": "JavaScript string interpolation ${}",
            "scope": [
                "punctuation.definition.template-expression.begin.js",
                "punctuation.definition.template-expression.begin.ts",
                "punctuation.definition.template-expression.end.ts",
                "punctuation.definition.template-expression.end.js"
            ],
            "settings": {
                "foreground": "#4dc4d9"
            }
        },
        {
            "scope": [
                "support.type.vendored.property-name",
                "support.type.property-name",
                "variable.css",
                "variable.scss",
                "variable.other.less"
            ],
            "settings": {
                "foreground": "#7ed6c6"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#c47f8f",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "keyword.control",
            "settings": {
                "foreground": "#c47f8f",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#ffffff",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "keyword.operator.new",
                "keyword.operator.expression",
                "keyword.operator.cast",
                "keyword.operator.sizeof",
                "keyword.operator.logical.python"
            ],
            "settings": {
                "foreground": "#4dc4d9"
            }
        },
        {
            "scope": "keyword.other.unit",
            "settings": {
                "foreground": "#7ed6c6"
            }
        },
        {
            "scope": "support.function.git-rebase",
            "settings": {
                "foreground": "#7ed6c6"
            }
        },
        {
            "scope": "constant.sha.git-rebase",
            "settings": {
                "foreground": "#7ed6c6"
            }
        },
        {
            "name": "coloring of the Java import and package identifiers",
            "scope": [
                "storage.modifier.import.java",
                "variable.language.wildcard.java",
                "storage.modifier.package.java"
            ],
            "settings": {
                "foreground": "#7ed6c6"
            }
        },
        {
            "name": "coloring of the TS this",
            "scope": "variable.language.this",
            "settings": {
                "foreground": "#4dc4d9"
            }
        },
        {
            "name": "Function declarations",
            "scope": [
                "entity.name.function",
                "support.function",
                "support.constant.handlebars",
                "variable.function"
            ],
            "settings": {
                "foreground": "#f5f5c8"
            }
        },
        {
            "name": "Types declaration and references",
            "scope": [
                "meta.return-type",
                "support.class",
                "support.type",
                "entity.name.type",
                "entity.name.class",
                "source.cs storage.type"
            ],
            "settings": {
                "foreground": "#9ad6ff"
            }
        },
        {
            "name": "Types declaration and references, TS grammar specific",
            "scope": [
                "meta.type.cast.expr",
                "meta.type.new.expr",
                "support.constant.math",
                "support.constant.dom",
                "support.constant.json",
                "entity.other.inherited-class"
            ],
            "settings": {
                "foreground": "#9ad6ff"
            }
        },
        {
            "name": "Control flow keywords",
            "scope": "keyword.control",
            "settings": {
                "foreground": "#c47f8f",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Variable and parameter name",
            "scope": [
                "variable",
                "meta.definition.variable.name",
                "support.variable"
            ],
            "settings": {
                "foreground": "#7ed6c6"
            }
        },
        {
            "name": "Object keys, TS grammar specific",
            "scope": [
                "meta.object-literal.key",
                "meta.object-literal.key entity.name.function"
            ],
            "settings": {
                "foreground": "#c7dde6"
            }
        },
        {
            "name": "CSS property value",
            "scope": [
                "support.constant.property-value",
                "support.constant.font-name",
                "support.constant.media-type",
                "support.constant.media",
                "constant.other.color.rgb-value",
                "constant.other.rgb-value",
                "support.constant.color"
            ],
            "settings": {
                "foreground": "#d4a584"
            }
        },
        // Python-specific tokens for improved language support
        {
            "scope": [
                "source.python",
                "storage.type.string.python"
            ],
            "settings": {
                "foreground": "#9ad6ff"
            }
        },
        {
            "scope": "meta.member.access.python",
            "settings": {
                "foreground": "#7ed6c6"
            } // matches variable/parameter name
        },
        {
            "scope": "meta.function-call.arguments.python",
            "settings": {
                "foreground": "#f5f5c8"
            } // matches function declarations
        },
        {
            "scope": "meta.item-access.python",
            "settings": {
                "foreground": "#c7dde6"
            } // matches object keys
        },
        {
            "scope": "variable.language.special.self.python",
            "settings": {
                "foreground": "#7ed6c6"
            }
        },
        {
            "scope": "meta.attribute.python",
            "settings": {
                "foreground": "#c47f8f"
            }
        },
        {
            "scope": "meta.function-call.generic.python",
            "settings": {
                "foreground": "#f5f5c8"
            }
        },
        {
            "name": "number",
            "scope": "constant.numeric.dec.python",
            "settings": {
                "foreground": "#d4a5cc"
            }
        },
        {
            "name": "string",
            "scope": [
                "string.quoted.single.python",
                "punctuation.definition.string.end.python",
                "punctuation.definition.string.begin.python"
            ],
            "settings": {
                "foreground": "#d4a584"
            }
        },
        {
            "scope": "constant.language.python",
            "settings": {
                "foreground": "#7ed6c6"
            }
        }
    ]
}