    "tokenColors": [
        {
            "scope": "emphasis",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "strong",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": "meta.diff.header",
            "settings": {
                "foreground": "#4dc4d9"
            }
        },
        {
            "scope": [
                "comment",
                "string.quoted.docstring.multi.python",
                "punctuation.definition.string.begin.python",
                "punctuation.definition.string.end.python",
                "string.quoted.docstring.raw.multi.python"
            ],
            "settings": {
                "foreground": "#6a7a8a",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "constant.language",
            "settings": {
                "foreground": "#4dc4d9"
            }
        },
        {
            "scope": [
                "constant.numeric",
                "constant.other.color.rgb-value",
                "constant.other.rgb-value",
                "support.constant.color"
            ],
            "settings": {
                "foreground": "#d4a5cc"
            }
        },
        {
            "scope": "constant.regexp",
            "settings": {
                "foreground": "#f5f5c8"
            }
        },
        {
            "name": "HTML tags",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#9ad6ff"
            }
        },
        {
            "scope": "entity.name.tag.css",
            "settings": {
                "foreground": "#f5f5c8"
            }
        },
        {
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#f5f5c8"
            }
        },
        {
            "scope": [
                "source.css entity.other.attribute-name",
                "source.css.less entity.other.attribute-name.id",
                "source.scss entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#f5f5c8"
            }
        },
        {
            "scope": "invalid",
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "scope": "markup.underline",
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": "markup.heading",
            "settings": {
                "foreground": "#9ad6ff"
            }
        },
        {
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#7ed6c6"
            }
        },
        {
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#c47f8f"
            }
        },
        {
            "scope": "markup.changed",
            "settings": {
                "foreground": "#4dc4d9"
            }
        },
        {
            "scope": "meta.selector",
            "settings": {
                "foreground": "#f5f5c8"
            }
        },
        {
            "name": "brackets of XML/HTML tags",
            "scope": [
                "punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#c7dde6"
            }
        },
        {
            "scope": "meta.preprocessor",
            "settings": {
                "foreground": "#569CD6"
            }
        },
        {
            "scope": "meta.preprocessor.string",
            "settings": {
                "foreground": "#c47f8f"
            }
